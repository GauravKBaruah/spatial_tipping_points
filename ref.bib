	note = {Section: New Results
Type: article},
	pages = {2022.03.07.483267},
	file = {Full Text PDF:/Users/gaurav/Zotero/storage/KTFWU43Y/Baruah et al. - 2022 - Community structure determines the predictability .pdf:application/pdf;Snapshot:/Users/gaurav/Zotero/storage/QMXU8BH8/2022.03.07.483267v1.html:text/html},
