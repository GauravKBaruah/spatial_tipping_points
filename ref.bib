	journal = {Ecology Letters},
	author = {Bell, Graham and Gonzalez, Andrew},
	year = {2009},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1461-0248.2009.01350.x},
	keywords = {Adaptation, environmental change, microcosm, experimental evolution, population, Saccharomyces cerevisiae},
	pages = {942--948},
	file = {Full Text PDF:/Users/gaurav/Zotero/storage/L74XB3V6/Bell and Gonzalez - 2009 - Evolutionary rescue can prevent extinction followi.pdf:application/pdf;Snapshot:/Users/gaurav/Zotero/storage/QEVESR2E/j.1461-0248.2009.01350.html:text/html},
