	note = {\_eprint: https://esajournals.onlinelibrary.wiley.com/doi/pdf/10.1890/12-0502.1},
	keywords = {growth, survival, meta-analysis, anti-predator behavior, nonconsumptive effects},
	pages = {573--579},
	file = {Full Text PDF:/Users/gaurav/Zotero/storage/E4XBJCFD/Orrock et al. - 2013 - The cost of safety Refuges increase the impact of.pdf:application/pdf;Snapshot:/Users/gaurav/Zotero/storage/JQZWIZC3/12-0502.html:text/html},
