@article{gellner_duality_2016-1,
	title = {The duality of stability: towards a stochastic theory of species interactions},
	issn = {1208001603032},
	url = {http://dx.doi.org/10.1007/s12080-016-0303-2},
	doi = {10.1007/s12080-016-0303-2},
	abstract = {Understanding the dynamics of ecological systems using stability concepts has been a key driver in ecological research from the inception of the field. Despite the tremendous effort put into this area, progress has been limited due to the bewildering number of metrics used to describe ecological stability. Here, we seek to resolve some of the confusion by unfolding the dynamics of a simple consumer-resource interaction module. In what follows, we first review common dynamical metrics of stability (CV, eigenvalues). We argue using the classical type II consumer-resource model as an example where the empirical stability metric, CV, hides two different, but important, aspects of stability: (i) stability due to mean population density processes and (ii) stability due to population density variance processes. We then employ a simple stochastic consumer-resource framework in order to elucidate (i) when we expect these two different aspects of stability to arise in ecological systems and, importantly, highlight (ii) the fact that these two different aspects of stability respond differentially, but predictably, to changes in fundamental parameters that govern biomass flux and loss in any consumer-resource interaction (e.g., attack rates, carrying capacity, mortality).},
	journal = {Theoretical Ecology},
	author = {Gellner, Gabriel and McCann, Kevin S. and Hastings, Alan},
	year = {2016},
	keywords = {Stability, Stochasticity, Consumer-Resource Dynamics, Transients},
	pages = {1--9},
	file = {Gellner, McCann, Hastings - 2016 - The duality of stability towards a stochastic theory of species interactions:/Users/gaurav/Zotero/storage/CFVV52EJ/Gellner, McCann, Hastings - 2016 - The duality of stability towards a stochastic theory of species interactions.pdf:application/pdf},
}

@article{dsouza_forecasting_2015-1,
	title = {Forecasting bifurcations from large perturbation recoveries in feedback ecosystems},
	volume = {10},
	doi = {10.1371/journal.pone.0137779},
	abstract = {Forecasting bifurcations such as critical transitions is an active research area of relevance to the management and preservation of ecological systems. In particular, anticipating the distance to critical transitions remains a challenge, together with predicting the state of the system after these transitions are breached. In this work, a new model-less method is presented that addresses both these issues based on monitoring recoveries from large perturbations. The approach uses data from recoveries of the system from at least two separate parameter values before the critical point, to predict both the bifurcation and the post-bifurcation dynamics. The proposed method is demonstrated, and its performance evaluated under different levels of measurement noise, with two ecological models that have been used extensively in previous studies of tipping points and alternative steady states. The first one considers the dynamics of vegetation under grazing; the second, those of macrophyte and phytoplankton in shallow lakes. Applications of the method to more complex situations are discussed together with the kinds of empirical data needed for its implementation.},
	number = {9},
	journal = {PLoS ONE},
	author = {D'Souza, Kiran and Epureanu, Bogdan I. and Pascual, Mercedes},
	year = {2015},
	pages = {1--19},
	file = {D'Souza, Epureanu, Pascual - 2015 - Forecasting bifurcations from large perturbation recoveries in feedback ecosystems:/Users/gaurav/Zotero/storage/PXSIKYDI/D'Souza, Epureanu, Pascual - 2015 - Forecasting bifurcations from large perturbation recoveries in feedback ecosystems.pdf:application/pdf},
}

@article{thattai_stochastic_2004-1,
	title = {Stochastic gene expression in fluctuating environments},
	volume = {167},
	issn = {0016-6731},
	doi = {10.1534/genetics.167.1.523},
	abstract = {Stochastic mechanisms can cause a group of isogenic bacteria, each subject to identical environmental conditions, to nevertheless exhibit diverse patterns of gene expression. The resulting phenotypic subpopulations will typically have distinct growth rates. This behavior has been observed in several contexts, including sugar metabolism and pili phase variation. Under fixed environmental conditions, the net growth rate of the population is maximized when all cells are of the fastest growing phenotype, so it is unclear what fitness advantage is conferred by population heterogeneity. However, unlike ideal laboratory conditions, natural environments tend to fluctuate, either periodically or randomly. Here we use a stochastic population model to show that, during growth in such fluctuating environments, a dynamically heterogenous bacterial population can sometimes achieve a higher net growth rate than a homogenous one. By using stochastic mechanisms to sample several distinct phenotypes, the bacteria are able to anticipate and take advantage of sudden changes in their environment. However, this heterogeneity is beneficial only if the bacterial response rate is sufficiently low. Our results could be useful in the design of artificial evolution experiments and in the optimization of fermentation processes.},
	number = {1},
	journal = {Genetics},
	author = {Thattai, Mukund and Van Oudenaarden, Alexander},
	year = {2004},
	pages = {523--530},
	file = {Thattai, Van Oudenaarden - 2004 - Stochastic gene expression in fluctuating environments:/Users/gaurav/Zotero/storage/M5TK4LCN/Thattai, Van Oudenaarden - 2004 - Stochastic gene expression in fluctuating environments.pdf:application/pdf},
}

@article{hefley_statistical_2013-3,
	title = {Statistical indicators and state-space population models predict extinction in a population of bobwhite quail},
	volume = {6},
	issn = {1874-1738},
	doi = {10.1007/s12080-013-0195-3},
	abstract = {Early warning systems of extinction thresholds have been developed for{\textbackslash}nand tested in microcosm experiments, but have not been applied to{\textbackslash}npopulations of wild animals. We used state-space population models and a{\textbackslash}nstatistical indicator to detect a transcritical bifurcation extinction{\textbackslash}nthreshold in a population of bobwhite quail (Colinus virginianus){\textbackslash}nlocated in an agricultural region experiencing habitat deterioration and{\textbackslash}nloss. The extinction threshold was detectible using two independent data{\textbackslash}nsets. We compared predictions from state-space population models to{\textbackslash}npredictions from a statistical indicator and found that predictions were{\textbackslash}ncorroborated. Using state-space population models, we estimated that our{\textbackslash}nstudy population crossed the extinction threshold in 2010 (2002-2036; 95{\textbackslash}n\% confidence intervals \{[\}CI]) using the whistle count (WC) data set{\textbackslash}nand in 2008 (1999-2064; 95 \% CI) using the Breeding Bird Survey (BBS){\textbackslash}ndata. With the statistical indicator, we estimated that the extinction{\textbackslash}nthreshold will be crossed in 2018 (2004-2031; 95 \% CI) using the WC{\textbackslash}ndata and will be crossed in 2012 (2006-2018; 95 \% CI) using the BBS{\textbackslash}ndata. We expect extinction in our study population soon after crossing{\textbackslash}nthe extinction threshold, but the time to extinction and potential{\textbackslash}nreversibility of the threshold are unknown. Our results suggest that{\textbackslash}nneither small nor decreasing population size will warn of the{\textbackslash}ntranscritical bifurcation extinction threshold. We suggest that managers{\textbackslash}nof wildlife populations in regions experiencing land use change should{\textbackslash}ntry to predict extinction thresholds and make management decisions to{\textbackslash}nensure the persistence of the species.},
	number = {3},
	journal = {Theoretical Ecology},
	author = {Hefley, Trevor J. and Tyre, Andrew J. and Blankenship, Erin E.},
	year = {2013},
	keywords = {Extinction, Bobwhite quail, Colinus virginianus, State-space model, Statistical indicator, Transcritical bifurcation},
	pages = {319--331},
	file = {Hefley, Tyre, Blankenship - 2013 - Statistical indicators and state-space population models predict extinction in a population of bob(2):/Users/gaurav/Zotero/storage/EXTFAE4R/Hefley, Tyre, Blankenship - 2013 - Statistical indicators and state-space population models predict extinction in a population of bob(2).pdf:application/pdf},
