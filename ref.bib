	pages = {2039--2046},
	file = {Full Text PDF:/Users/gaurav/Zotero/storage/VDSC75GS/Vázquez et al. - 2009 - Evaluating multiple determinants of the structure .pdf:application/pdf;Snapshot:/Users/gaurav/Zotero/storage/C2AXHDXB/08-1837.html:text/html},
