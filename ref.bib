@article{ergon_when_2015-1,
	title = {When three traits make a line: {Evolution} of phenotypic plasticity and genetic assimilation through linear reaction norms in stochastic environments},
	url = {http://biorxiv.org/lookup/doi/10.1101/034256},
	doi = {10.1101/034256},
	abstract = {Running title: When three traits make a line Abstract 1 Genetic assimilation results from selection on phenotypic plasticity, but quantitative genetics models 2 of linear reaction norms considering intercept and slope as traits do not fully incorporate the process of 3 genetic assimilation. We argue that intercept-slope reaction norm models are insufficient 4 representations of genetic effects on linear reaction norms, and that defining the intercept as a trait is 5 unfortunate. Instead we suggest a model with three traits representing genetic effects that respectively 6 (1) are independent of the environment, (2) alter the sensitivity of the phenotype to the environment, 7 and (3) determine how the organism perceives the environment. The model predicts that, given 8 sufficient additive genetic variation in environmental perception, the environmental value at which 9 reaction norms tend to cross will respond rapidly to selection, and eventually become equal to the 10 mean environment. Hence, in this model, genetic assimilation in a new environment becomes 11 complete without changes in genetic correlations, genetic drift or imposing any fitness costs on 12 maintaining plasticity. The asymptotic evolutionary outcome of this three-trait linear reaction norm 13 generally entails a lower degree of phenotypic plasticity than the two-trait model, and maximum 14 expected fitness does not occur at the mean trait values in the population.},
	number = {1},
