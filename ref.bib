	issn = {1461-0248},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1461-0248.2010.01490.x},
	doi = {10.1111/j.1461-0248.2010.01490.x},
	abstract = {Ecology Letters (2010) 13: 989–997 Abstract Adaptive variation in the traits determining ecological interactions can lead to evolution so rapid that ecological dynamics change course while in progress (i.e., ‘eco-evolutionary dynamics’). However, little is known about how the qualitative properties of eco-evolutionary dynamics (e.g., cycling, equilibrium, etc.) are affected by the amount of heritable variation present. Here, we show that a change in the range of variation in a heritable prey defense trait determines what dynamics are observed in an experimental predator–prey system. We combine modelling and laboratory experiments to show that initial defense trait variation determines whether populations exhibit eco-evolutionary cycles in which heritable variation is maintained, or converge to an equilibrium at which the prey population becomes monomorphic. Our results show how small changes in the amount of adaptive genetic variance initially present can radically alter eco-evolutionary dynamics, and can ultimately determine whether heritable variation is maintained or lost.},
	language = {en},
	number = {8},
	urldate = {2021-09-12},
	journal = {Ecology Letters},
	author = {Becks, Lutz and Ellner, Stephen P. and Jones, Laura E. and Jr, Nelson G. Hairston},
	year = {2010},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1461-0248.2010.01490.x},
	keywords = {eco-evolutionary dynamics, predator–prey dynamics, Adaptive variation, Brachionus, Chlamydomonas, evolutionary cycles, genetic variability, polymorphism},
	pages = {989--997},
}

@article{limberger_spatial_2019,
	title = {Spatial insurance in multi-trophic metacommunities},
	volume = {22},
	issn = {1461-0248},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/ele.13365},
	doi = {10.1111/ele.13365},
	abstract = {Metacommunity theory suggests that dispersal is a key driver of diversity and ecosystem functioning in changing environments. The capacity of dispersal to mitigate effects of environmental change might vary among trophic groups, potentially resulting in changes in trophic interactions and food web structure. In a mesocosm experiment, we compared the compositional response of bacteria, phyto- and zooplankton to a factorial manipulation of acidification and dispersal. We found that the buffering capacity of dispersal varied among trophic groups: dispersal alleviated the negative effect of acidification on phytoplankton diversity mid-experiment, but had no effect on the diversity of zooplankton and bacteria. Likewise, trophic groups differed in whether dispersal facilitated compositional change. Dispersal accelerated changes in phytoplankton composition under acidification, possibly mediated by changes in trophic interactions, but had no effect on the composition of zooplankton and bacteria. Overall, our results suggest that the potential for spatial insurance can vary among trophic groups.},
	language = {en},
	number = {11},
	urldate = {2021-09-15},
	journal = {Ecology Letters},
	author = {Limberger, Romana and Pitt, Alexandra and Hahn, Martin W. and Wickham, Stephen A.},
	year = {2019},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/ele.13365},
	keywords = {metacommunity, environmental change, Bacteria, food web, trophic interactions, zooplankton, dispersal, experiment, mesocosms, phytoplankton},
	pages = {1828--1837},
