	keywords = {eco-evolutionary dynamics, community genetics, Biodiversity and ecosystem functioning, ecological stoichiometry, ecosystem science, evolutionary biology, feedbacks, natural selection},
	pages = {690--701},
	file = {Full Text PDF:/Users/gaurav/Zotero/storage/BI24U5S8/Matthews et al. - 2011 - Toward an integration of evolutionary biology and .pdf:application/pdf;Snapshot:/Users/gaurav/Zotero/storage/YAE453J6/j.1461-0248.2011.01627.html:text/html},
