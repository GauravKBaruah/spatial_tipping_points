	url = {https://esajournals.onlinelibrary.wiley.com/doi/abs/10.1890/08-1673.1},
	doi = {https://doi.org/10.1890/08-1673.1},
	abstract = {Studies of adaptive divergence have traditionally focused on the ecological causes of trait diversification, while the ecological consequences of phenotypic divergence remain relatively unexplored. Divergence in predator foraging traits, in particular, has the potential to impact the structure and dynamics of ecological communities. To examine the effects of predator trait divergence on prey communities, we exposed zooplankton communities in lake mesocosms to predation from either anadromous or landlocked (freshwater resident) alewives, which have undergone recent and rapid phenotypic differentiation in foraging traits (gape width, gill raker spacing, and prey size-selectivity). Anadromous alewives, which exploit large prey items, significantly reduced the mean body size, total biomass, species richness, and diversity of crustacean zooplankton relative to landlocked alewives, which exploit smaller prey. The zooplankton responses observed in this experiment are consistent with patterns observed in lakes. This study provides direct evidence that phenotypic divergence in predators, even in its early stages, can play a critical role in determining prey community structure.},
	language = {en},
	number = {2},
	urldate = {2020-12-12},
	journal = {Ecology},
	author = {Palkovacs, Eric P. and Post, David M.},
	year = {2009},
	note = {\_eprint: https://esajournals.onlinelibrary.wiley.com/doi/pdf/10.1890/08-1673.1},
	keywords = {intraspecific variation, adaptive divergence, zooplankton, alewife, Alosa pseudoharengus, size-selective predation, eco-evolutionary interactions, extended phenotype, gill rakers, niche construction, phenotypic divergence, predator–prey interactions},
	pages = {300--305},
	file = {Full Text PDF:/Users/gaurav/Zotero/storage/IX4KTK7S/Palkovacs and Post - 2009 - Experimental evidence that phenotypic divergence i.pdf:application/pdf;Snapshot:/Users/gaurav/Zotero/storage/ETJGV923/08-1673.html:text/html},
