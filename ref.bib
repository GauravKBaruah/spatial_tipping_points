	note = {Publisher: Royal Society},
	keywords = {Food webs, Conservation, Pollination, Nestedness, Redundancy, Generalization},
	pages = {2605--2611},
	file = {Full Text PDF:/Users/gaurav/Zotero/storage/35VCIHM4/Memmott et al. - 2004 - Tolerance of pollination networks to species extin.pdf:application/pdf},
}

