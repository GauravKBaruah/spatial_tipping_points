	number = {3},
	urldate = {2021-04-18},
	journal = {Biological Conservation},
	author = {Beger, Maria and Grantham, Hedley S. and Pressey, Robert L. and Wilson, Kerrie A. and Peterson, Eric L. and Dorfman, Daniel and Mumby, Peter J. and Lourival, Reinaldo and Brumbaugh, Daniel R. and Possingham, Hugh P.},
	month = mar,
	year = {2010},
	keywords = {Conservation decision-support systems, Environmental realms, Integrated conservation, Realm connectivity, Systematic conservation planning},
	pages = {565--575},
	file = {ScienceDirect Snapshot:/Users/gaurav/Zotero/storage/RYBUFNUY/S0006320709004716.html:text/html},
}

@article{holt_food_2002,
