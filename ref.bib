@article{scheffer_early-warning_2009-2,
	title = {Early-warning signals for critical transitions.},
	volume = {461},
	url = {http://dx.doi.org/10.1038/nature08227%5Cn%3CGo to ISI%3E://WOS:000269478800026%5Cnhttp://www.nature.com/nature/journal/v461/n7260/pdf/nature08227.pdf},
