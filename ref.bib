	file = {Accepted Version:/Users/gaurav/Zotero/storage/RW52SUE7/Anderson and Hayes - 2018 - The effects of dispersal and river spatial structu.pdf:application/pdf;Snapshot:/Users/gaurav/Zotero/storage/A67GX4EL/fwb.html:text/html},
