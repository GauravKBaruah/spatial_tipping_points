	author = {DeLong, John P. and Gilbert, Benjamin and Shurin, Jonathan B. and Savage, Van M. and Barton, Brandon T. and Clements, Christopher F. and Dell, Anthony I. and Greig, Hamish S. and Harley, Christopher D. G. and Kratina, Pavel and McCann, Kevin S. and Tunney, Tyler D. and Vasseur, David a. and O’Connor, Mary I.},
	year = {2015},
	keywords = {allometry, body size scaling, food chain, interaction, predator-prey size ratio, strength, trophic cascade},
	pages = {354--366},
	file = {DeLong et al. - 2015 - The Body Size Dependence of Trophic Cascades:/Users/gaurav/Zotero/storage/STC9U7EP/DeLong et al. - 2015 - The Body Size Dependence of Trophic Cascades.pdf:application/pdf},
