	note = {Publisher: American Association for the Advancement of Science
Section: Research Article},
	pages = {eaav0474},
	file = {Full Text PDF:/Users/gaurav/Zotero/storage/ER4RT2IM/Link and Watson - 2019 - Global ecosystem overfishing Clear delineation wi.pdf:application/pdf;Snapshot:/Users/gaurav/Zotero/storage/SGXM5W4L/eaav0474.html:text/html},
