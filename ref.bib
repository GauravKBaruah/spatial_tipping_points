}

@article{thomas_extinction_2004-1,
	title = {Extinction risk from climate change.},
	volume = {427},
	issn = {0028-0836},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/18653892},
	doi = {10.1038/nature02121},
	abstract = {Climate change over the past approximately 30 years has produced numerous shifts in the distributions and abundances of species and has been implicated in one species-level extinction. Using projections of species' distributions for future climate scenarios, we assess extinction risks for sample regions that cover some 20\% of the Earth's terrestrial surface. Exploring three approaches in which the estimated probability of extinction shows a power-law relationship with geographical range size, we predict, on the basis of mid-range climate-warming scenarios for 2050, that 15-37\% of species in our sample of regions and taxa will be 'committed to extinction'. When the average of the three methods and two dispersal scenarios is taken, minimal climate-warming scenarios produce lower projections of species committed to extinction ( approximately 18\%) than mid-range ( approximately 24\%) and maximum-change ( approximately 35\%) scenarios. These estimates show the importance of rapid implementation of technologies to decrease greenhouse gas emissions and strategies for carbon sequestration.},
	number = {6970},
	journal = {Nature},
	author = {Thomas, Chris D and Thomas, Chris D and Cameron, Alison and Cameron, Alison and Green, Rhys E and Green, Rhys E and Bakkenes, Michel and Bakkenes, Michel and Beaumont, Linda J and Beaumont, Linda J and Collingham, Yvonne C and Collingham, Yvonne C and Erasmus, Barend F N and Erasmus, Barend F N and De Siqueira, Marinez Ferreira and De Siqueira, Marinez Ferreira and Grainger, Alan and Grainger, Alan and Hannah, Lee and Hannah, Lee and Hughes, Lesley and Hughes, Lesley and Huntley, Brian and Huntley, Brian and Van Jaarsveld, Albert S and Van Jaarsveld, Albert S and Midgley, Guy F and Midgley, Guy F and Miles, Lera and Miles, Lera and Ortega-Huerta, Miguel a and Ortega-Huerta, Miguel a and Peterson, a Townsend and Peterson, a Townsend and Phillips, Oliver L and Phillips, Oliver L and Williams, Stephen E and Williams, Stephen E},
