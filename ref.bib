	keywords = {speciation, species interactions, coevolution, birds},
	pages = {5608--5610},
	file = {Full Text PDF:/Users/gaurav/Zotero/storage/AQ9ZFLDF/Thompson - 2016 - Coevolution, local adaptation and ecological speci.pdf:application/pdf;Snapshot:/Users/gaurav/Zotero/storage/EXDU59VE/mec.html:text/html},
