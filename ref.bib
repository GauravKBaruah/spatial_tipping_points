	file = {Connell, Ghedini - 2015 - Resisting regime-shifts The stabilising effect of compensatory processes:/Users/gaurav/Zotero/storage/VYNG4XBK/Connell, Ghedini - 2015 - Resisting regime-shifts The stabilising effect of compensatory processes.pdf:application/pdf},
}

@article{mathematical_theory_of_communication_shannon_mathematical_1948,
