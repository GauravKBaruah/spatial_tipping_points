	keywords = {facilitation, competition, food webs, mutualism, positive interaction, ecological networks, ecological communities, multiplex networks, negative interaction, rocky intertidal},
	pages = {291--303},
	file = {Snapshot:/Users/gaurav/Zotero/storage/9FQXUVQX/13-1424.html:text/html;Snapshot:/Users/gaurav/Zotero/storage/FQQGD9UN/13-1424.html:text/html;Submitted Version:/Users/gaurav/Zotero/storage/MBQSFLV6/Kéfi et al. - 2015 - Network structure beyond food webs mapping non-tr.pdf:application/pdf},
